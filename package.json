{
  "name": "monster-slayer",
  "version": "0.1.0",
  "description": "A Fruit Ninja-style monster slicing game with RPG progression",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "keywords": [
    "game",
    "phaser",
    "typescript",
    "monster",
    "slasher",
    "arcade"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.80.1",
    "@supabase/supabase-js": "^2.46.2"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "vite": "^5.4.21",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "@eslint/js": "^9.17.0",
    "prettier": "^3.4.2"
  }
}
