{
  "name": "monster-slayer",
  "version": "0.1.0",
  "description": "A Fruit Ninja-style monster slicing game with RPG progression",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky",
    "notion:sync-docs": "node scripts/notion/sync-docs.cjs",
    "notion:sync-commit": "node scripts/notion/sync-commit.cjs",
    "notion:sync-game-data": "node scripts/notion/sync-game-data.cjs",
    "notion:sync-todos": "node scripts/notion/sync-todos.cjs",
    "notion:create-task": "node scripts/notion/create-task.cjs",
    "notion:sync-all": "npm run notion:sync-docs && npm run notion:sync-game-data && npm run notion:sync-todos",
    "notion:update": "npm run notion:sync-docs && npm run notion:sync-commit"
  },
  "keywords": [
    "game",
    "phaser",
    "typescript",
    "monster",
    "slasher",
    "arcade"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.46.2",
    "phaser": "^3.80.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@notionhq/client": "^5.6.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "inquirer": "^13.1.0",
    "prettier": "^3.4.2",
    "terser": "^5.44.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.21"
  }
}
